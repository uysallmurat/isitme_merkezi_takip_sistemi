{
  "name": "isitme-merkezi-frontend",
  "version": "1.0.0",
  "description": "İşitme Merkezi Takip Sistemi Frontend",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:error-handler": "jest error-handler.test.js",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/static/frontend/js/$1"
    },
    "collectCoverageFrom": [
      "static/frontend/js/**/*.js",
      "!static/frontend/js/tests/**",
      "!static/frontend/js/vendor/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/*.test.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "babel-jest": "^29.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0"
  },
  "dependencies": {},
  "keywords": [
    "hearing-center",
    "management-system",
    "django",
    "frontend"
  ],
  "author": "İşitme Merkezi Takip Sistemi",
  "license": "MIT"
} 